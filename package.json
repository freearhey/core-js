{
  "name": "@freearhey/core",
  "version": "0.7.0",
  "description": "Toolset for Node.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npx tsc --build",
    "lint": "npx eslint ./src/**/*.ts ./test/**/*.ts",
    "test": "run-script-os",
    "test:win32": "SET \"TZ=Pacific/Nauru\" && jest --runInBand",
    "test:default": "TZ=Pacific/Nauru jest --runInBand"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/freearhey/core-js.git"
  },
  "author": "Arhey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/freearhey/core-js/issues"
  },
  "homepage": "https://github.com/freearhey/core-js#readme",
  "dependencies": {
    "@types/fs-extra": "^11.0.2",
    "@types/lodash": "^4.14.198",
    "@types/luxon": "^3.3.2",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.4",
    "lodash": "^4.17.21",
    "luxon": "^3.4.3",
    "natural-orderby": "^3.0.2",
    "node-gzip": "^1.1.2",
    "normalize-url": "^6.1.0",
    "object-treeify": "^2.1.1",
    "run-script-os": "^1.1.6",
    "signale": "^1.4.0"
  },
